{
  "manifest_version": 3,
  "name": "OneTab FOSS",
  "version": "1.0.0",
  "description": "An open-source OneTab alternative - Save and manage your tabs efficiently",
  "icons": {
    "96": "icons/icon-96.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "contextMenus"
  ],
  "background": {
    "scripts": ["src/background/background.js"],
    "type": "module"
  },
  "action": {
    "default_popup": "src/popup/popup.html",
    "default_icon": {
      "96": "icons/icon-96.png"
    },
    "default_title": "OneTab FOSS"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "onetab-foss@example.com",
      "strict_min_version": "109.0"
    }
  }
}
